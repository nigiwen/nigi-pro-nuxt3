    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.16",
    "@unocss/eslint-config": "^0.58.5",
    "@unocss/nuxt": "^0.58.5",
    "@vite-pwa/nuxt": "^0.4.0",
    "@vueuse/nuxt": "^10.7.2",
    "ant-design-vue": "^4.1.2",
    "commitizen": "^4.3.0",
