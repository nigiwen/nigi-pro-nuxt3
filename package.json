    "pnpm": "^8.15.1",
    "sass": "^1.70.0",
    "taze": "^0.13.3",
    "typescript": "^5.3.3",
    "unocss-preset-useful": "^0.4.0",
    "vue-draggable-plus": "^0.3.5",
    "vue-tsc": "^1.8.27"
