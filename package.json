{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.12.1",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s gen:css build:nuxt",
    "build:nuxt": "nuxt build",
    "dev": "run-s gen:css dev:nuxt",
    "dev:nuxt": "nuxt dev",
    "generate": "run-s gen:css generate:nuxt",
    "generate:nuxt": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "gen:css": "esno scripts/genAntdStyle.ts",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install",
    "commit": "git add -A && czg && git push",
    "dedupe": "pnpm dedupe",
    "up": "taze major -I"
  },
  "devDependencies": {
    "@ant-design-vue/nuxt": "^1.2.0",
    "@antfu/eslint-config": "^2.4.6",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@nuxt/devtools": "latest",
    "@pinia/nuxt": "^0.5.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.5",
    "@unocss/eslint-config": "^0.58.0",
    "@unocss/nuxt": "^0.58.0",
    "@vite-pwa/nuxt": "^0.3.6",
    "@vueuse/nuxt": "^10.7.0",
    "commitizen": "^4.3.0",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "cz-git": "^1.8.0",
    "czg": "^1.8.0",
    "eslint": "npm:eslint-ts-patch@8.55.0-1",
    "eslint-plugin-format": "^0.1.0",
    "eslint-ts-patch": "8.55.0-1",
    "esno": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nanoid": "^5.0.4",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.8.2",
    "pinia": "^2.1.7",
    "pnpm": "^8.12.1",
    "sass": "^1.69.5",
    "taze": "^0.13.0",
    "typescript": "^5.3.3",
    "unocss-preset-useful": "^0.3.5",
    "vue-draggable-plus": "^0.3.2",
    "vue-tsc": "^1.8.25"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
